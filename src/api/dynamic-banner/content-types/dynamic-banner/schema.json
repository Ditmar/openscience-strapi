{
  "kind": "collectionType",
  "collectionName": "dynamic_banners",
  "info": {
    "singularName": "dynamic-banner",
    "pluralName": "dynamic-banners",
    "displayName": "Dynamic-Banner"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "allowedTypes": [
        "images",
        "videos"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "redirect": {
      "type": "string",
      "required": true,
      "regex": "^https?:\\/\\/[\\w\\-]+(\\.[\\w\\-]+)+[/#?]?.*$"
    }
  }
}
